	<% @games.each do |game| %>
		<div class="container" id="reviewBlock">
			<div class="row">
				<div class="col-md-6">
					<h3>
						<a href="/<%= game[:id]%>"><%= game[:title]%></a>

					</h3>
				</div>
				<div class="col-md-6">

					<p><%= game[:review]%></p>
				</div>
			</div>
			
		</div>
	<% end %>
